page {
    background-color: #f8f9fb;
}

.swiper {
    height: 380rpx;
}

.swiper image {
    width: 100%;
    height: 100%;
}

.content {
    display: flex;
    flex-wrap: wrap;
    border-top: 1rpx solid #eee;
    border-left: 1rpx solid #eee;
}

.content .content-item {
    width: 33.3333333%;
    height: 250rpx;
    display: flex;
    border-top: 1rpx solid #eee;
    border-right: 1rpx solid #eee;
    box-sizing: border-box;
    justify-content: center;
    flex-direction: column;
    align-items: center;
    background-color: #fff;
}

.content .content-item image {
    width: 70rpx;
    height: 70rpx;
}

.content .content-item .info {
    font-size: 28rpx;
    color: #888;
}

.links {
    display: flex;
    background-color: #fff;
    margin: 10rpx 0;
    padding: 30rpx;
    justify-content: space-between;
}

.links image {
    display: block;
    width: 330rpx;
    height: 185rpx;
}